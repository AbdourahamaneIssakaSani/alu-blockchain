{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/blockchain.h",
                "${workspaceFolder}/*.c",
                "${workspaceFolder}/provided/*.h",
                "${workspaceFolder}/provided/_blockchain_destroy.c",
                "${workspaceFolder}/provided/_blockchain_print.c",
                "${workspaceFolder}/provided/_endianness.c",
                "-Wall",
                "-Werror",
                "-Wextra",
                "-pedantic",
                "-I${workspaceFolder}",
                "-L${workspaceFolder}/../../crypto",
                "-lhblk_crypto",
                "-lssl",
                "-lllist",
                "-lcrypto",
                "-pthread",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}